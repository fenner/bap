OBJS=	parser.o scanner.o main.o @LIBOBJS@

DEBUG=	-g
CFLAGS=	@DEFS@ -DYYDEBUG ${DEBUG}
YACC=	@YACC@ -v

bap:	${OBJS}
	${CC} -o $@ ${DEBUG} ${OBJS} -ll

scanner.c:	scanner.l
parser.c:	parser.y

clean:
	rm -f bap scanner.c parser.c $(OBJS)
