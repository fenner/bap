OBJS=	parser.o scanner.o main.o @LIBOBJS@

D=	-g
CFLAGS=	@DEFS@ -DYYDEBUG ${D}
YACC=	@YACC@ -v

bap:	${OBJS}
	${CC} -o $@ ${D} ${OBJS} -ll

scanner.c:	scanner.l
parser.c:	parser.y

clean:
	rm -f bap scanner.c parser.c $(OBJS)
